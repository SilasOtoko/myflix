<div>
  <h1><%= @category.name %></h1>
  <br/>
  <div>
    <% @category.videos.each do |video| %>
      <%= link_to video_path(video) do %>
        <img src="<%= video.small_cover %>" class="small-cover">
        <br/>
        <h1><%= video.title %></h1>
      <% end %>
    <% end %>
  </div>
</div>